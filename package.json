{
  "name": "template-web-app",
  "version": "0.1.0",
  "description": "Template for Web Application",
  "scripts": {
    "start": "npm run start-dev",
    "start-dev": "webpack serve --mode=development --config=configs/webpack/dev.js --port 9090 --host 0.0.0.0",
    "start-prod": "http-server ./dist -p 9080",
    "storybook": "start-storybook --config-dir ./configs/storybook -p 9070",
    "build": "yarn clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.7",
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "babel-loader": "8.2.2",
    "css-loader": "5.0.1",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.0.0-alpha.6",
    "http-server": "0.12.3",
    "node-sass": "5.0.0",
    "prettier": "2.2.1",
    "react-hot-loader": "4.13.0",
    "sass-loader": "10.1.0",
    "source-map-loader": "1.1.2",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.11",
    "typescript": "4.1.2",
    "url-loader": "4.1.1",
    "webpack": "5.9.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.4.0"
  },
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "autoprefixer": "^9.8.6",
    "mini-css-extract-plugin": "1.3.3",
    "postcss": "^7.0.35",
    "postcss-cli": "8.3.1",
    "postcss-import": "12.0.0",
    "postcss-loader": "4.1.0",
    "postcss-preset-env": "6.7.0",
    "postcss-scss": "^3.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
  },
  "optionalDependencies": {
    "@storybook/addon-a11y": "6.1.9",
    "@storybook/addon-actions": "6.1.9",
    "@storybook/addon-essentials": "6.1.9",
    "@storybook/addon-links": "6.1.9",
    "@storybook/react": "6.1.9",
    "@storybook/source-loader": "6.1.9"
  }
}
